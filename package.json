{
  "name": "hauki-admin-ui",
  "version": "0.1.0",
  "license": "MIT",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/enzyme": "^3.10.6",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.5",
    "axios": "^0.20.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "eslint-config-airbnb-typescript-prettier": "^3.1.0",
    "hds-core": "^0.13.1",
    "hds-react": "^0.13.0",
    "prettier": "^2.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-test-renderer": "^16.13.1",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-cypress": "start-server-and-test start http://localhost:3000 cypress-run-chrome",
    "test-cypress-headless": "start-server-and-test start http://localhost:3000 cypress-run-chrome-headless",
    "eject": "react-scripts eject",
    "lint": "eslint --ext js,ts,tsx src",
    "cypress-open": "cypress open",
    "cypress-run-chrome": "cypress run --browser chrome",
    "cypress-run-chrome-headless": "cypress run --headless --browser chrome"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!hds-react)/"
    ]
  },
  "devDependencies": {
    "cypress": "^5.1.0",
    "start-server-and-test": "^1.11.3"
  }
}
